function changecity()
{
 var xb=document.all.sheng.options[document.all.sheng.selectedIndex].value;
 
 //alert(home);
 if (xb!="0")
 {
  index = document.all.sheng.options.selectedIndex-1;
  document.all.shi.length = city[index].length;
  for(var i = 0;i<city[index].length;i++)
  {
   var text = city[index][i].split("-");
   document.all.shi.options[i].text = text[0];
   document.all.shi.options[i].value =text[1];
  }

//  xb=document.all.shi.options.selectedIndex;
//  document.all.homes.length=xian[xb].length;
  //for(var j=0;j<xian[xb].length;j++)
 // {
   //var txt=xian[xb][j].split("-");
   //document.all.homes.options[j].text=txt[0];
  // document.all.homes.options[j].value=txt[1];
   
  //}
 }
 //var homes=document.all.shi.options[document.all.shi.selectedIndex].value;
 //if (home!="0")
 //{
  //var number=document.all.shi.options[document.all.shi.selectedIndex].value-1;
  //document.all.homes.length=xian[number].length;
  //for(var i=0;i<xian[number].length;i++)
  //{
   //var txt=xian[number].split("-");
   //document.all.homes.options[i].value=txt[0];
   //document.all.homes.options[i].text=txt[1];
  //}
 //}
}
